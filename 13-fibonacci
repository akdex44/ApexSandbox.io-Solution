public Integer fibonacci(Integer n) {
    if (n <= 0){

    }
    if (n == 1) || (n == 2){
           return 1;     
    } 

    Integer val1 = 1;
    Integer val2 = 2;
    Integer current = 0;

    for(Integer i = 3; i <= n; i++){
        current = val1 + val2;
        val1 = val2;
        val2 = current;
        
    } 
    return val2;
}