public Integer fibonacci(Integer n) {
        
        if (n < 3) {
            return 1;
        }
        
        Integer previous = 1;
        Integer current = 1;
        Integer next = 0;

        for (Integer i = 2; i < n; i++) {
            next = previous + current; 
            previous = current; 
            current = next; 
        }
        
        return current;
    }
}

   